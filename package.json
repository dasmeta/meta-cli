{
  "author": "hovhannes.babayan @hb90dev",
  "bin": {
    "meta": "./bin/run.js"
  },
  "dependencies": {
    "@aws-sdk/client-cloudwatch": "^3.441.0",
    "@aws-sdk/client-cloudwatch-logs": "^3.441.0",
    "@aws-sdk/client-ec2": "^3.441.0",
    "@aws-sdk/client-eks": "^3.441.0",
    "@aws-sdk/client-elastic-load-balancing-v2": "^3.441.0",
    "@aws-sdk/client-iam": "^3.490.0",
    "@aws-sdk/client-rds": "^3.441.0",
    "@aws-sdk/client-s3": "^3.445.0",
    "@dagrejs/graphlib-dot": "^1.0.2",
    "@kubernetes/client-node": "^0.20.0",
    "@oclif/core": "^3",
    "@oclif/plugin-help": "^6.0.10",
    "@oclif/plugin-plugins": "^4.1.15",
    "@types/cli-progress": "^3.11.5",
    "@types/inquirer": "8",
    "axios": "^1.5.1",
    "chalk": "4",
    "eslint-config-prettier": "^9.0.0",
    "ini": "^4.1.1",
    "inquirer": "^8",
    "lodash": "^4.17.21",
    "sift": "^17.0.1",
    "yaml": "^2.3.3"
  },
  "description": "cli tool for managing dasmeta resources",
  "devDependencies": {
    "@aws-sdk/types": "^3.433.0",
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^3",
    "@types/chai": "^4",
    "@types/ejs": "^3.1.3",
    "@types/ini": "^1.3.31",
    "@types/mocha": "^9.0.0",
    "@types/node": "^18",
    "chai": "^4",
    "eslint": "^8.51.0",
    "eslint-config-oclif": "^5",
    "eslint-config-oclif-typescript": "^3",
    "mocha": "^10",
    "oclif": "^4.2.0",
    "shx": "^0.3.3",
    "ts-node": "^10.9.1",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/dasmeta/meta-cli",
  "license": "MIT",
  "main": "dist/index.js",
  "name": "@dasmeta/meta-cli",
  "oclif": {
    "autocomplete": {
      "refreshCache": true
    },
    "bin": "meta",
    "dirname": "meta",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "repository": "dasmeta/meta-cli",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts",
    "postinstall": "./bin/run.js autocomplete",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "version": "0.0.0",
  "bugs": "https://github.com/dasmeta/meta-cli/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
